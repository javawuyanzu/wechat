
.navbar{
  flex: none;
  margin-top: 10px;
  display: flex;
  background: #fff;
}
.image{
  padding-top: 20px;
 width: 30px;
 height: 25px;
}
.navbar .item{
  margin-left: 10px;
  padding-top: 20px;
  position: relative;
  flex: auto;
 
  line-height: 80rpx;
}
.quenyButton1{
 background-color:#67c23a;
 color: white;
 margin-top: 40rpx;
 font-size: 30rpx;
 height: 10%;
  width: 70%;
   margin-left: 15%;  
}

.select_box1{
  z-index:250;
  
   display: flex;
 background: #fff;
 width: 100%;
 margin: 30rpx auto;
 position: relative;
}
.select1{
  
 box-sizing: border-box;
 width: 55%;
 height: 70rpx;
 border:1px solid #efefef;
 border-radius: 8rpx;
 display: flex;
 align-items: center;
 padding: 0 20rpx;
 
}
.select_text1{
 
 font-size: 30rpx;
 flex: 1;
}
.select_img1{
  
 width: 40rpx;
 height: 40rpx;
 display: block;
 transition:transform 0.3s;
}
.select_img_rotate1{
 
 transform:rotate(180deg); 
}
.option_box1{
   
  margin-left: 26%;
 position: absolute;
 top: 70rpx;
 width: 55%;
 box-sizing: border-box;
 height: 0;
 overflow-y: auto;
 border-top: 0;
 background: #fff;
 transition: height 0.3s;
}
.option1{
 display: block;
 line-height: 40rpx;
 font-size: 30rpx;
 padding: 10rpx;
}
