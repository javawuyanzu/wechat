					</view>
				</view>
			</navigator>
		</view>
		<view wx:if="{{sole}}">
			<navigator url="../soldUpdate/soldUpdate" hover-class="none">
				<view class="user-list-item">
					<view class="item-left">
						<image style="width: 42rpx; height: 42rpx" src="../../../images/collect-kong.png"></image>
