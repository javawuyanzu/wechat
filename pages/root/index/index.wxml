			</view>
		</view>
	</view>

	<view class="user-list">


		<view class="user-list-item">
			<view class="item-left">
				<image style="width: 42rpx; height: 42rpx" src="../../../images/collect-kong.png"></image>
				<text>{{content.index_exType}}</text>
			</view>
			<view class="item-right" style="line-height: 0rpx;font-size: 16px;color: #333;">
				<picker bindchange="bindPickerExChange" value="{{exindex}}" range="{{exarray}}">
