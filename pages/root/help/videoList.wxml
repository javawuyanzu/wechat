<view class="page" data-weui-theme="{{theme}}">
    <view class="page__hd">
        <view class="page__title">{{engTitle}}</view>
        <view class="page__desc">{{title}}</view>
    </view>
    <view class="page__bd" >
        <view class="weui-cells weui-cells_after-title"   >
            <navigator url="/pages/root/video/video?title={{item.title}}}&src={{item.src}}"  class="weui-cell weui-cell_access" hover-class="weui-cell_active"  wx:for="{{videoList}}" wx:key="index" wx:for-item="item">
                <view class="weui-cell__hd">
                    <image src="../../../images/video1.png" style="margin-right: 16px;vertical-align: middle;width:30px; height: 30px;"></image>
                </view>
                <view class="weui-cell__bd">{{item.title}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"> 点击播放</view>
            </navigator>
        </view>
    </view>
</view>
